<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:aop="http://www.springframework.org/schema/aop"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
    http://www.springframework.org/schema/aop 
    http://www.springframework.org/schema/aop/spring-aop-3.0.xsd ">

    
    

   <!-- Definition for student bean -->
   <bean  id="circle1" class="Circle">
      <property name="name"  value="Zara" />      
   </bean>
   
   <bean  id="circle" class="Circle">
      <property name="name"  value="WARAS" />      
   </bean>

   <bean  id="triangle" class="Triangle">
      <property name="name"  value="Yara" />      
   </bean>
   
         
  
    <bean  name="shape" class="Shape" autowire="byName" >
     
    </bean>
   
   <!-- Definition for logging aspect -->
   <!-- <bean id="logging" class="Logging"/> -->
   
   <bean class="org.springframework.aop.aspectj.annotation.AnnotationAwareAspectJAutoProxyCreator"></bean>  
      
</beans>
<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

<bean id="ds" class="org.springframework.jdbc.datasource.DriverManagerDataSource">  
 <property name="driverClassName" value="com.mysql.jdbc.Driver" />  
 <property name="url" value="jdbc:mysql://localhost/test" />  
 <property name="username" value="root" />  
 <property name="password" value="password" />  
</bean>  

<bean id="jdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate">  
 <property name="dataSource" ref="ds"></property>  
</bean>  

<bean id="edao" class="EmpDAO">  
 <property name="template" ref="jdbcTemplate"></property>  
</bean> 

<bean id="propertieshandler" class="PropertiesHandler" init-method="init" destroy-method="destroy"> 
</bean>

<bean id="templateBean"  abstract="true">
 <property name="a" value = "100"/>
 <property name="b" value= "200"/>
</bean>

<bean id="templateImpl"  class="TemplateImpl" parent = "templateBean">
</bean>

<bean id="address"  class="Address">

<property name="street" value="Ahalyabai"></property>
<property name="village" value="Mirapota"></property>
</bean>

<bean id="employee"  class="Employee">
<property name="name" value="Amal"></property>
<property name="addr" ref = "address"></property>
</bean>

<bean id="testHi"  class="TestHi">
<property name="name" value="Hiiiii"></property>
</bean>

<bean id="person"  class="Person">
<property name="a">
<list>

<value>Hello</value>
</list>
</property>
</bean>


<bean id="hi"  class="Address">
<property name="street" value ="Rockford"></property>
<property name="village" value ="mirapota">
</property>
</bean>

<bean id="person1"  class="Person1">
<property name="b">
<list>
   <value>Aife</value>
   <value>Saife</value>
   <value>Anirban</value>
</list>
</property>
</bean>



</beans>  